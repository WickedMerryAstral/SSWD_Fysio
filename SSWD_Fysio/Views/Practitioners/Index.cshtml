@model IEnumerable<Core.Domain.Practitioner>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.studentNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.employeeNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BIGNumber)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.studentNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.employeeNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BIGNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.practitionerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.practitionerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.practitionerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
